                    "closeButton": true,
                    "positionClass": "toast-top-right"
                }
                toastr.success("WP request has been saved");
            }

            var submitted = @((TempData["Submitted"] != null).ToString().ToLower());
            if (submitted == true) {
                toastr.options = {
                    "closeButton": true,
                    "positionClass": "toast-top-right"
                }
                toastr.success("WP request has been submitted");
            }

