@{
    ViewBag.Title = "Network Number Page | Import";
}
@section breadcrumb{
    <li>
        <a href="@Url.Action("Index")">Network Number List</a>
        <i class="fa fa-circle"></i>
    </li>
    <li>
        <span>Import</span>
    </li>
}

<h1 class="page-title">Network Number <small>Import</small></h1>
<div class="portlet light portlet-fit portlet-form bordered">
    <div class="portlet-title">
        <div class="caption">
            <span class="caption-subject font-blue sbold uppercase">Network Number Import Form</span>
        </div>
        <div class="pull-right">
            <a class="btn btn-default" href="@Url.Action("Index")"><i class="fa fa-arrow-circle-left"></i> Return to index</a>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="portlet-body">
        <div class="row">

            <div class="col-lg-12">
                <form role="form" asp-action="Import" method="post" id="form" class="form-horizontal" enctype="multipart/form-data">
                    <div class="form-group">
                        <div class="col-md-6 control-label">
                            <input type="file" style="margin-left:10px;" class="control-label" name="file" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" required />
                        </div>
                        <div class="col-md-6">
                            <input type="submit" class="btn btn-primary" value="Upload" />
                        </div>
                    </div>
                </form>
                <hr />
            </div>

            <div class="col-lg-12">
                <div class="col-lg-12">
                    <div class="table-responsive">
                        <p style="margin-left:10px;" class="pull-left"><strong>Format Import ( Example ) :</strong></p>
						<p style="margin-left:10px;" class="pull-right"><small><i><strong>Note : Organizational Unit and Account Name must be registered</strong></i></small></p>
                        <table class="table table-bordered">
                            <tr>
                                <td></td>
                                <td>A</td>
                                <td>B</td>
                                <td>C</td>
                                <td>D</td>
                                <td>E</td>
                                <td>F</td>
                                <td>G</td>
                            </tr>
                            <tr>
                                <td>1</td>
                                <td>Network Number</td>
                                <td>Project</td>
                                <td>Organizational Unit</td>
                                <td>Line Manager</td>
                                <td>Total Project Manager</td>
                                <td>Account Name</td>
                                <td>Description</td>
                            </tr>
                            <tr>
                                <td>2</td>
                                <td>94257007</td>
                                <td>XL Central Java RO 2014</td>
                                <td>RASO KAM Telkom</td>
                                <td>puspita.oetami@ericsson.com</td>
                                <td>toni.chandra@ericsson.com</td>
                                <td>XL AXIATA</td>
                                <td>ISAT BSC RNC 2015</td>
                            </tr>
                            <tr>
                                <td>3</td>
                                <td>94257010</td>
                                <td>ISAT CORE AOP 2014</td>
                                <td>RASO Supply</td>
                                <td>marta.rosdiana@ericsson.com</td>
                                <td>dedhi.udiyarsa@ericsson.com</td>
                                <td>INDOSAT</td>
                                <td>ISAT BSC RNC 2015</td>
                            </tr>
                            <tr>
                                <td colspan="7" align="left"><strong>Worksheet 1</strong></td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>


        </div>
    </div>
</div>

@section Scripts {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/assets/pages/scripts/role/form-validation.js"></script>
    <script>
        $(document).ready(function () {
            $common.setMenu("#menu-master-data", "#menu-network-number");
            FormValidation.init("#form");
        });
    </script>
}